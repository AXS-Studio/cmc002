  padding: .2em .6em .3em .6em;
  width: auto;
  overflow: visible; }

/* The close button */
.ui-timepicker .ui-timepicker-close {
  float: right; }

/* the now button */
.ui-timepicker .ui-timepicker-now {
  float: left; }

/* the deselect button */
.ui-timepicker .ui-timepicker-deselect {
  float: left; }

/* IE6 IFRAME FIX (taken from datepicker 1.5.3 */
